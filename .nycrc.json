{
  "extends": "@istanbuljs/nyc-config-typescript",
  "all": true,
  "check-coverage": true,
  "reporter": ["text", "html", "lcov"],
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/**/*.test.ts",
    "src/**/*.spec.ts",
    "test/**/*",
    "out/test/**/*",
    "**/*.d.ts"
  ],
  "branches": 0,
  "lines": 0,
  "functions": 0,
  "statements": 0
}
